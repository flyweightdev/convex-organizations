{
  "name": "@flyweightdev/convex-organizations",
  "version": "0.1.16",
  "description": "Convex component for organizations, RBAC, invitations, user profiles, device management, audit logging, and admin impersonation",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flyweightdev/convex-organizations.git"
  },
  "homepage": "https://github.com/flyweightdev/convex-organizations#readme",
  "bugs": {
    "url": "https://github.com/flyweightdev/convex-organizations/issues"
  },
  "keywords": [
    "convex",
    "organizations",
    "rbac",
    "multi-tenancy",
    "invitations",
    "audit-logging",
    "user-management"
  ],
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "types": "./dist/client/index.d.ts",
  "module": "./dist/client/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@convex-dev/component-source": "./src/client/index.ts",
      "types": "./dist/client/index.d.ts",
      "default": "./dist/client/index.js"
    },
    "./admin": {
      "@convex-dev/component-source": "./src/client/admin.ts",
      "types": "./dist/client/admin.d.ts",
      "default": "./dist/client/admin.js"
    },
    "./providers": {
      "@convex-dev/component-source": "./src/providers/index.ts",
      "types": "./dist/providers/index.d.ts",
      "default": "./dist/providers/index.js"
    },
    "./react": {
      "@convex-dev/component-source": "./src/react/index.ts",
      "types": "./dist/react/index.d.ts",
      "default": "./dist/react/index.js"
    },
    "./convex.config": {
      "@convex-dev/component-source": "./src/component/convex.config.ts",
      "types": "./dist/component/convex.config.d.ts",
      "default": "./dist/component/convex.config.js"
    },
    "./_generated/component.js": {
      "types": "./dist/component/_generated/component.d.ts"
    }
  },
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json",
    "build:codegen": "npx convex codegen --component-dir ./src/component && npm run build",
    "build:clean": "rm -rf dist *.tsbuildinfo && npm run build:codegen",
    "prepublishOnly": "npm run build:clean",
    "predev": "path-exists .env.local dist || (npm run build && convex dev --once)",
    "dev": "run-p -r 'dev:*'",
    "dev:backend": "convex dev --typecheck-components",
    "dev:build": "chokidar 'tsconfig*.json' 'src/**/*.ts' 'src/**/*.tsx' -i '**/*.test.ts' -c 'npm run build:codegen' --initial"
  },
  "peerDependencies": {
    "@convex-dev/auth": ">=0.0.90",
    "convex": ">=1.31.0",
    "react": ">=18.0.0",
    "resend": ">=6.0.0",
    "twilio": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "resend": {
      "optional": true
    },
    "twilio": {
      "optional": true
    }
  },
  "devDependencies": {
    "@convex-dev/auth": "^0.0.90",
    "@types/node": "^25.0.0",
    "@types/react": "^19.2.0",
    "@types/twilio": "^3.19.2",
    "chokidar-cli": "^3.0.0",
    "convex": "^1.31.0",
    "npm-run-all2": "^8.0.0",
    "path-exists-cli": "^2.0.0",
    "react": "^19.2.0",
    "resend": "^6.9.2",
    "twilio": "^5.12.1",
    "typescript": "~5.9.0"
  }
}
